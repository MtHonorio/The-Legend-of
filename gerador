#include<stdio.h>
#include <conio.h>
#include <stdlib.h>// necessário p/ as funções rand() e srand()
#include<time.h>//necessário p/ função time()
int main(void)
{
  int i, tempo, numerosGerados;

  printf("Gerando 10 valores aleatorios:\n\n");



  srand(time(NULL));     /*INICIA GERADOR DE NÚMEROS DA FUNÇÃO TIME, ASSIM, NÃO SERÁ GERADO
                           O MESMO VALOR SEMPRE AO INICIAR O PROGRAMA*/

  puts("Digite quantos numero voce quer que seja gerado: ");
  scanf("%d", &numerosGerados);
  puts("O intervalo que pode ser gerado e de zero a? ");
  scanf("%d", &tempo);

  for (i=0; i < numerosGerados; i++) /* "i < N" = DE I ATÉ N */
  {
    // GERAR N VALORES ALEATÓRIOS DE ZERO À TEMPO(entrada do usuário)
    printf("%d ", rand() % tempo);
  }

  getch();
  return 0;
}
